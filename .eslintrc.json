{
    "env": {
        "browser": true,
        "node": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 0,
        "no-undef": 0,
        "no-unused-vars": [
            "error", {
                "args": "none"
            }
        ],
        "no-redeclare": 0
    }
}
